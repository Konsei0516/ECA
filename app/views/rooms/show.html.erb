<h1 class="title-room">Chat room</h1>
<div class="col-md-12">
  <div class="chat_box">
    <div id = "messages">
      <ul class = "list-group">
        <%= render @messages %>
      </ul>
    </div>
  </div>
    <form class="form-inline">
      <div class ="form-group">
        <input type="text" class="form-control" id="chat-input"/>
      </div>
      <button id="button" class="btn btn-primary">送信</button>
    </form>
  </div>
</div>
